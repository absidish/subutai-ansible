{
  "name": "SubutaiAnsible",
  "description": "Environment configuration test blueprint",
  "nodes": [
    {
      "name": "backup-server",
      "template": "ubuntu16",
      "peer_group": "HTTP-GROUP",
      "size": "TINY"
    }
  ],
  "peerGroups": [
    {
      "name": "HTTP-GROUP",
      "pricingPref": {
        "maxPrice": "50"
      }
    }
  ],
  "sshKey": "root",
  "configuration": {
    "playbook": "https://github.com/platium/subutai-ansible/archive/master.zip",
    "script": "main.yml",
    "groups": [
      {
        "name": "db",
        "nodes_name": [
          "backup-server"
        ]
      }
    ]
  }
}
